{
    "name": "easybiblabs/oauth2-client-php",
    "description": "OAuth2 client for PHP",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Yitzchak Schaffer",
            "email": "yitzchak@imagineeasy.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/http-foundation": "3.3.*"
    },
    "require-dev": {
      "phpunit/phpunit": "4.8.*",
      "phpmd/phpmd": "^2.2.3",
      "squizlabs/php_codesniffer": "^2.3.3"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "EasyBib\\Guzzle\\": "src/",
            "EasyBib\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/"],
        "psr-0": {
            "EasyBib\\Tests\\OAuth2\\Client\\": "tests/"
        }
    }
}
