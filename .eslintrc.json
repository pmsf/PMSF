{
  "extends": "standard",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
      }
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2
      }
    ]
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "toastr": true,
    "google": true,
    "RichMarker": true,
    "RichMarkerPosition": true,
    "mapData": true,
    "Store": true,
    "centerLat": true,
    "centerLng": true,
    "locationSet": true,
    "zoom": true,
    "minZoom": true,
    "maxLatLng": true,
    "showConfig": true,
    "pageLoaded": true,
    "countMarkers": true,
    "skel": true,
    "setupPokemonMarker": true,
    "createjs": true,
    "Push": true,
    "isTouchDevice": true,
    "isMobileDevice": true,
    "pokemonSprites": true,
    "noLabelsStyle": true,
    "darkStyle": true,
    "light2Style": true,
    "pGoStyle": true,
    "darkStyleNoLabels": true,
    "light2StyleNoLabels": true,
    "pGoStyleNoLabels": true,
    "pGoStyleDay": true,
    "pGoStyleNight": true,
    "moment": false,
    "mapStyle": true,
    "directionProvider" : true,
    "hidePokemon": true,
    "excludeMinIV": true,
    "minIV": true,
    "minLevel": true,
    "showBigKarp":true,
    "showTinyRat":true,
    "hidePokemonCoords": true,
    "notifyPokemon": true,
    "notifyRarity": true,
    "notifyIv": true,
    "notifyLevel": true,
    "notifyRaid": true,
    "enableRaids": true,
    "activeRaids": true,
    "minRaidLevel": true,
    "maxRaidLevel": true,
    "enableGyms": true,
    "gymSidebar": true,
    "enablePokemon": true,
    "enablePokestops": true,
    "enableLured": true,
    "showWeather": true,
    "enableWeatherOverlay": true,
    "enableScannedLocations": true,
    "enableSpawnpoints": true,
    "enableRanges": true,
    "notifySound": true,
    "criesSound": true,
    "enableStartMe": true,
    "enableStartLast": true,
    "enableFollowMe": true,
    "enableSpawnArea": true,
    "iconSize": true,
    "iconNotifySizeModifier": true,
    "locationStyle": true,
    "gymStyle": true,
    "spriteFileLarge": true,
    "weatherSpritesSrc": true,
    "icons": true,
    "getIv": true,
    "mapType": true,
    "triggerGyms": true,
    "i8ln": false,
    "languageSite": true,
    "boostedMons": true
  }
}
